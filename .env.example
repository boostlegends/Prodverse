# Suno AI API
SUNO_API_KEY=your_suno_api_key_here

# Optional: Webhook URL for Suno callbacks (set this to your deployed URL)
# SUNO_CALLBACK_URL=https://yourdomain.com/api/suno/webhook

# Optional: App URL for callbacks
# NEXT_PUBLIC_APP_URL=https://yourdomain.com

# Google Sheets Waitlist (get this from Google Apps Script deployment)
GOOGLE_SHEETS_WEBHOOK_URL=your_google_apps_script_url_here

# Supabase (get these from your Supabase project settings > API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
